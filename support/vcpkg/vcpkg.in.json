	"name": "@PROJECT_NAME_LOWER@",
	"version": "@PROJECT_VERSION@",
	"maintainers": [
		"@PROJECT_VENDOR@"
	],
	"description": [
		"@PROJECT_DESCRIPTION@"
	],
	"documentation": "@PROJECT_HOMEPAGE_URL@/README.rst",
	"homepage": "@PROJECT_HOMEPAGE_URL@",
	"dependencies": [
		{
			"name": "vcpkg-cmake",
			"host": true
